Algoritmo "crescente"
//
Var
   x, y: inteiro


Inicio
      escreva("Digite dois numeros: ")
      leia(x)
      leia(y)

      enquanto x <> y faca
      se x < y entao
      escreval("Ordem crescente.")
      senao
      escreval("ordem descrescente.")
      fimse
      
      escreval("Digite outros dois numeros: ")
      leia(x)
      leia(y)
      fimenquanto



Fimalgoritmo